    private void jTextField1ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jTextField1ActionPerformed
        
        if ((jTextField1.getText().isEmpty())) {
            JOptionPane.showMessageDialog(null, "Serao Pesquisado Todos");
            DefaultTableModel tableModel = (DefaultTableModel) jTablePesquisa.getModel();
            tableModel.setNumRows(10);
        }

            List<Proprietario> receber = new ArrayList();

            try {
                UsuarioDAO b = new UsuarioDAO();
                receber = b.procuraUser("%" + jTextField1.getText() + "%");
                if (receber.size() == 0) {
                    DefaultTableModel tableModel = (DefaultTableModel) jTablePesquisa.getModel();
                    int n = tableModel.getRowCount();
                    for (int i = 0; i < n; i++) {
                        jTablePesquisa.setValueAt(null, i, 0);
                        jTablePesquisa.setValueAt(null, i, 1);
                        jTablePesquisa.setValueAt(null, i, 2);
                        jTablePesquisa.setValueAt(null, i, 3);
                    }
                    JOptionPane.showMessageDialog(null, "NAO ENCONTRADO");
                }
                for (int i = 0; i < receber.size(); ++i) {

                    DefaultTableModel tableModel = (DefaultTableModel) jTablePesquisa.getModel();
                    tableModel.setNumRows(receber.size());
                    jTablePesquisa.setValueAt(receber.get(i).getIdProprietario(), i, 0);
                    jTablePesquisa.setValueAt(receber.get(i).getNome(), i, 1);
                    jTablePesquisa.setValueAt(receber.get(i).getCpfcnpj(), i, 2);
                    jTablePesquisa.setValueAt(receber.get(i).getRg(), i, 3);
                }

            } catch (Exception ex) {
                Logger.getLogger(JDialogPProprietario.class.getName()).log(Level.SEVERE, null, ex);
                JOptionPane.showMessageDialog(null, ex.getMessage() + "\n");
            }

        
    }//GEN-LAST:event_jTextField1ActionPerformed
