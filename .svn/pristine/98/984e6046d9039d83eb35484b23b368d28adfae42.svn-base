    private void jTextField5ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jTextField5ActionPerformed
        Imovel imovel = new Imovel();
        imovel.setCI(jTextField5.getText());
        if ((jTextField5.getText().isEmpty())) {
            JOptionPane.showMessageDialog(null, "Os campos não podem retornar vazios");
        } else {
            try {
                ImovelDAO imo = null;
                try {
                    imo = new ImovelDAO();
                } catch (ClassNotFoundException ex) {
                    Logger.getLogger(JDialogPProprietario.class.getName()).log(Level.SEVERE, null, ex);
                    JOptionPane.showMessageDialog(null, "\n" + ex.getMessage() + "\n");
                }
                String Tipo = String.valueOf((String) jComboBox1.getSelectedItem());
                if (Tipo == "Selecione") {
                    JOptionPane.showMessageDialog(null, "SELECIONE UMA OPCAO");
                }
                if (Tipo == "Incra") {
                    incra = imo.procuraIDUser(jTextField5.getText());
                    if (incra == null) {
                        JOptionPane.showMessageDialog(null, "NAO ENCONTRADO");
                    } else {
                        JOptionPane.showMessageDialog(null, "ENCONTRADO");
                        jLabel12.setText(incra.getMatricula());
                        jLabel13.setText(incra.getNome());
                        jLabel14.setText(incra.getArea());
                        jLabel15.setText(incra.getTipoProcesso());
                    }
                }
                if (Tipo == "Outros") {
                    outros = imo.procuraIDUserO(jTextField5.getText());
                    if (outros == null) {
                        JOptionPane.showMessageDialog(null, "NAO ENCONTRADO");
                    } else {
                        JOptionPane.showMessageDialog(null, "ENCONTRADO");
                        jLabel12.setText(outros.getMatricula());
                        jLabel13.setText(outros.getNome());
                        jLabel14.setText(outros.getArea());
                        jLabel15.setText(outros.getTipoProcesso());
                    }
                }
                //jTextField3.setText(Nome); problemas
            } catch (HeadlessException | SQLException ex) {
                Logger.getLogger(JDialogPProprietario.class.getName()).log(Level.SEVERE, null, ex);
                JOptionPane.showMessageDialog(null, "\n" + ex.getMessage() + "\n");
            }

        }        // TODO add your handling code here:
    }//GEN-LAST:event_jTextField5ActionPerformed

    private void jButton6ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton6ActionPerformed
        if (jComboBox1.getSelectedItem().toString().equals("Selecione")) {
            JOptionPane.showMessageDialog(null, "ESCOLHA UMA TIPO DE IMOVEL");
        }
        if (jComboBox1.getSelectedItem().toString().equals("Outros")) {

            JOptionPane.showMessageDialog(null, "ESCOLHA UMA TIPO DE IMOVEL");
        }
        if (jComboBox1.getSelectedItem().toString().equals("Incra")) {
            JOptionPane.showMessageDialog(null, "ESCOLHA UMA TIPO DE IMOVEL");
