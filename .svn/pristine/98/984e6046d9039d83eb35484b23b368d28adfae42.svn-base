    private void jButton7ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton7ActionPerformed
        boolean inserido = false;
        serviços serv = new serviços();
        SimpleDateFormat formatador1 = new SimpleDateFormat("yyyy/MM/dd");
        Date Data = jDateChooser2.getDate();
        String data = formatador1.format(Data);
        serv.setID(jTextField3.getText());
        serv.setCI(jTextField5.getText());
        serv.setCod(jTextField1.getText());
        serv.setValor(jTextField2.getText() + "." + jTextField4.getText());
        serv.setData(data);
        serv.setDemais(jTextArea2.getText());
        String Tipo = String.valueOf((String) jComboBox1.getSelectedItem());
        System.out.println(data);
        if ((jTextField1.getText().isEmpty()) || (jTextField2.getText().isEmpty()) || (jTextField3.getText().isEmpty()) || (jTextField4.getText().isEmpty()) || (jTextField5.getText().isEmpty())) {
            JOptionPane.showMessageDialog(null, "Os campos não podem retornar vazios");
        } else {
            ServiçosDAO imo = null;
            try {
                imo = new ServiçosDAO();
            } catch (ClassNotFoundException ex) {
                Logger.getLogger(JDialogPProprietario.class.getName()).log(Level.SEVERE, null, ex);
                JOptionPane.showMessageDialog(null, "\n" + ex.getMessage() + "\n");
            }
            if (Tipo == "Selecione") {
                JOptionPane.showMessageDialog(null, "SELECIONE UMA OPCAO");
            }
            if (Tipo == "Incra") {
                inserido = imo.realizaincra(serv);
            }
            if (Tipo == "Outros") {
                inserido = imo.realizaoutros(serv);
            }
            if (inserido == true) {
                JOptionPane.showMessageDialog(null, jTextField1.getText() + "\nImovel: " + Tipo + " " + jTextField5.getText() + "\nInserido com sucesso! ");
            }
            
        }
        int showConfirmDialog = JOptionPane.showConfirmDialog(null, "Outro Cadastro?");
        if ((showConfirmDialog == 1)) {
            this.dispose();
        }// TODO add your handling code here:
    }//GEN-LAST:event_jButton7ActionPerformed

